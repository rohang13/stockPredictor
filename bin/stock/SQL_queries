CREATING STOCK-DETAILS TABLE.
create table STOCK_DETAILS (STOCK_iD number(10),STOCK_EXCHANGE_TYPE varchar2(10),STOCKNAME varchar2(20),PRICE number(10),NO_OF_STOCKS_BOUGHT number(10),STOCK_HOLDER_NAME varchar2(10),DATE_OF_CHANGE date default sysdate not null);

INSERTING VALUES IN STOCK_DETAILS TABLE
INSERT INTO STOCK_DETAILS(STOCK_iD ,STOCK_EXCHANGE_TYPE,STOCKNAME ,PRICE ,NO_OF_STOCKS_BOUGHT ,STOCK_HOLDER_NAME ) VALUES(101,'BSE','TCS',1027,15,'VENKATESAN');